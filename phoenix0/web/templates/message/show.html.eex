<h2>Show message</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @message.title %>
  </li>

  <li>
    <strong>Receiver:</strong>
    <%= @message.receiver %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @message.body %>
  </li>

  <li>
    <strong>Priority:</strong>
    <%= @message.priority %>
  </li>

</ul>

<%= link "Edit", to: message_path(@conn, :edit, @message) %>
<%= link "Back", to: message_path(@conn, :index) %>
